{
  "name": "exchange-price-service",
  "version": "1.0.0",
  "description": "Binance Exchange Price Service",
  "main": "index.js",
  "license": "PRIVATE",
  "private": true,
  "scripts": {
    "precommit": "pretty-quick --staged",
    "start": "tsc && node dist/index.js",
